<?xml version ="1.0" encoding ="utf-8"?><!--  Learn More about how to use com.william.reservationsystem.App Actions: https://developer.android.com/guide/actions/index.html -->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <!--点击效果-->
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <!-- 半透明圆角按钮 -->
            <solid android:color="#8FFFFFFF"/>
            <corners android:radius="8dip"/>
        </shape>
    </item>

    <item android:state_pressed="false">
        <!--未点击效果-->
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <!-- 透明圆角按钮 -->
            <solid android:color="#1FFFFFFF"/>
            <corners android:radius="8dip"/>
        </shape>
    </item>
</selector>